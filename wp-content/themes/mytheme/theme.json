      }
    },
    "blocks": {
      "core/button": {
        "color": {
          "gradient": "linear-gradient(#a8ff78, #78ffd6)",
          "text": "rgb(17 24 39)"
        }
      }
    },
    "typography": {
      "fontFamily": "Rubik, sans-serif",
      "fontSize": "16px",
      "fontStyle": "normal",
      "fontWeight": "normal",
      "lineHeight": "inherit",
      "textDecoration": "none",
      "textTransform": "none"
    }
