      "dropCap": true,
      "fontWeight": true,
      "fontStyle": true,
      "textTransform": true,
      "textDecoration": true,
      "letterSpacing": true
    },
    "layout": {
      "contentSize": "900px",
      "wideSize": "1100px"
    },
    "spacing": {
      "margin": true,
      "padding": true
    }
